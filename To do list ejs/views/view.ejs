<%- include("header")-%>
<body>
    <div class="box" id="heading">
        <h1><%= listtitle%></h1>
    </div>

    <div class="box">
        <ul>
            <% for(let i=0; i<newListItems.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%=newListItems[i]%>></p>
            </div>
            <%}%>
        </ul>

        <form class="item" action="/" method="post">
            <input placeholder="New Item" type="text" name="newItem" autocomplete="off">
            <button type="submit" name="list" value=<% listtitle %>>>+</button>
        </form>
    </div>
<%- include("footer")-%>